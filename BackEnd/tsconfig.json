{
  "compilerOptions": {
     "module": "commonjs",
      "target": "es5",
      "lib": ["es2018", "dom"],
      "moduleResolution": "node",
      "removeComments": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true
  }
}